<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ImageEditText">
        <attr name="ie_left_text" format="string"></attr>
        <!-- 左侧显示图片-->
        <attr name="ie_left_img" format="reference"></attr>
        <attr name="ie_text" format="string"></attr>
        <attr name="ie_hint_text" format="string"></attr>
        <!--EditText底部样式 -->
        <attr name="ie_et_bg" format="reference"></attr>
        <attr name="ie_have_del" format="boolean"></attr>
        <attr name="ie_have_pwd" format="boolean"></attr>
        <attr name="ie_edit_line" format="integer"></attr>
        <attr name="num" format="boolean"></attr>
    </declare-styleable>
    <declare-styleable name="PullScrollView">
        <attr name="header" format="reference" />
        <attr name="headerHeight" format="dimension" />
        <attr name="headerVisibleHeight" format="dimension" />
    </declare-styleable>

</resources>